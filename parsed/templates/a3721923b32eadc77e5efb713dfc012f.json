{"components/products/options/set-rectangle":"<div class=\"form-field show-selected-option\" data-product-attribute=\"set-rectangle\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{this.display_name}}:\r\n        <span data-option-value></span>\r\n\r\n        {{#if required}}\r\n            <small class=\"required-label\">{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_rectangle_{{../id}}_{{id}}\"\r\n            data-option-label=\"{{this.label}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}\r\n                checked\r\n                data-default\r\n            {{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label class=\"form-option\" for=\"attribute_rectangle_{{../id}}_{{id}}\" data-title=\"{{this.label}}\" data-product-attribute-value=\"{{id}}\">\r\n            <span class=\"form-option-variant\">{{this.label}}</span>\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n"}
