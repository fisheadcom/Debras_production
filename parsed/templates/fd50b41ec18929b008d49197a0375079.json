{"components/common/breadcrumbs":"<ul class=\"breadcrumbs\" itemscope itemtype=\"http://schema.org/BreadcrumbList\">\r\n    {{#each breadcrumbs}}\r\n        <li class=\"breadcrumb {{#if @last}}is-active{{#if ../../page_type '===' 'product'}} breadcrumb--product{{/if}}{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"http://schema.org/ListItem\">\r\n            {{#if @last}}\r\n                <meta itemprop=\"item\" content=\"{{url}}\">\r\n                <span class=\"breadcrumb-label\" itemprop=\"name\">\r\n                    {{#if ../../page_type '===' 'product'}}\r\n                        <span class=\"default-title {{#if ../../../product.custom_fields}}{{#each ../../../../product.custom_fields}}{{#if name '===' 'custom-title' }}hide{{/if}}{{/each}}{{/if}}\">{{../name}}</span>\r\n                        {{#if ../../../product.custom_fields}}\r\n                            {{#each ../../../../product.custom_fields}}\r\n                                {{#if name '===' 'custom-title' }}\r\n                                    <span class=\"custom-title\">{{{value}}}</span>\r\n                                {{/if}}\r\n                            {{/each}}\r\n                        {{/if}}\r\n                    {{else}}\r\n                        {{name}}\r\n                    {{/if}}\r\n                </span>\r\n            {{else}}\r\n                {{#if url}}\r\n                    <a href=\"{{url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">{{name}}</span></a>\r\n                {{else}}\r\n                    <meta itemprop=\"item\" content=\"{{url}}\">\r\n                    <span class=\"breadcrumb-label\" itemprop=\"name\">{{name}}</span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <meta itemprop=\"position\" content=\"{{@index}}\" />\r\n        </li>\r\n    {{/each}}\r\n</ul>\r\n"}
